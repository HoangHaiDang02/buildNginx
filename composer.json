{
    "name": "coccoc/php-with-docker-template",
    "description": "Template of PHP course 2022.",
    "keywords": ["framework"],
    "type": "project",
    "version": "v1.0.1",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-mysqli": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    }
}
